{
    "author": "Digit",
    "content_scripts": [ {
       "js": [ "content-script.js" ],
       "matches": [ "https://*.youtube.com/watch?*" ],
       "run_at": "document_start"
    } ],
    "description": "Speed-up video ads without adding elements to the page. Skip through ads with minimal impact to your browsing experience.",
    "icons": {
       "128": "icons/icon128.png",
       "16": "icons/icon16.png",
       "32": "icons/icon32.png"
    },
    "manifest_version": 2,
    "name": "Ad Speedup Lite - Lightweight Ad Skipping",
    "short_name": "Ad Speedup Lite",
    "version": "1.1.0",
    "browser_specific_settings": {
       "gecko": {
         "id": "ad-speedup-lite@bitwise.live"
       }
     }
 }
 